{
    "manifest_version": 3,
    "name": "InterfaceAI MVP",
    "version": "0.1.0",
    "description": "Minimal Chrome extension frontend for InterfaceAI",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["tabs"],
    "host_permissions": ["http://localhost:5000/*"]
  }

